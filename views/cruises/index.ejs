<%- include('../partials/header') %>

<h1>My Cruises</h1>

<section id="user-cruise-gallery">
  <% cruises.forEach(function(cruise, idx) { %>
    <div class="user-cruise-thumbnail" id="thumb-<%= idx %>">
      <p><%= cruises.title %></p>
      <p><%= cruises.startDate %> - <%= cruises.endDate %></p>
      <p><%= cruises.logs.length %> Logs</p>
    </div>
  <% }); %>
</section>

<%- include('../partials/footer') %>